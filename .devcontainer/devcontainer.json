{
	"name": "PyTorch",
	"build": {
		"dockerfile": "Dockerfile"
	},
	"runArgs": [
		"--gpus",
		"all"
	],
	"customizations": {
		"vscode": {
			"extensions": [
				"charliermarsh.ruff",
				"ms-azuretools.vscode-docker",
				"ms-python.python",
				"ms-toolsai.jupyter",
				"tamasfe.even-better-toml"
			]
		}
	},
	"postCreateCommand": "poetry install"
}
